                  <%= link_to 'More details', lactic_session_path(lactic_session), :class => 'new_osm' %>

                </h4>

                <!--</div>-->
              <!--</div>-->
              </div>

              <!--<div class="arrow"></div>-->
            </div>
          <!--</li>-->
        <% end %>
